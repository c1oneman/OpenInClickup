{
  "manifest_version": 3,
  "name": "Clickup Desktop Redirect",
  "description": "Swaps https://app.clickup.com URLS with clickup:// protocol",
  "version": "1.0",
  "icons": {
    "38": "images/icon-38.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://app.clickup.com/*"]
    }
  ],
  "permissions": ["tabs"]
}
